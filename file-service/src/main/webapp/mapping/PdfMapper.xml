<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wdy.module.modular.system.mapper.PdfMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wdy.module.modular.system.entity.Pdf">
        <id column="pdfid" property="pdfid"/>
        <result column="cover" property="cover"/>
        <result column="datetime" property="datetime"/>
        <result column="file_id" property="fileId"/>
        <result column="name" property="name"/>
        <result column="uid" property="uid"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        pdfid, cover, datetime, file_id AS fileId, name, uid
    </sql>
</mapper>
